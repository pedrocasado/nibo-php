{
    "name": "pedrocasado/nibo-php",
    "type": "library",
    "description": "Simple Nibo PHP wrapper",
    "keywords": [
        "nibo",
        "nibophp",
        "nibo-php-wrapper"
    ],
    "license": [
        "LGPL-3.0-or-later",
        "GPL-3.0-or-later",
        "MIT"
    ],
    "authors": [
        {
            "name": "Pedro Casado",
            "email": "pdr33n@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "ext-json": "*",
        "php": ">= 7.1",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "NiboPhp\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "v1.0": "1.0-dev"
        }
    },
    "minimum-stability": "stable"
}
